  "license": "MIT",
  "dependencies": {
    "dayjs": "^1.11.9",
    "epg-grabber": "^0.36.1",
    "epg-parser": "^0.2.0",
    "cwait": "^1.1.2",
    "curl-generator": "^0.2.0",
    "axios": "^0.21.1",
    "lodash": "^4.17.21",
    "iconv-lite": "^0.6.3",
    "cheerio": "^1.0.0-rc.12",
    "xml-js": "^1.6.11",
    "luxon": "^3.3.0"
  }
