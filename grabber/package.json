{
  "name": "epg",
  "version": "0.1.0",
  "scripts": {
    "update-codes": "node scripts/update-codes.js",
    "NpmInstall": "npm install --save",
    "indihometv_com": "epg-grabber --config=./config/indihometv.com.config.js --channels=./config/indihometv.com.channels.xml --output=./guide/indihometv.com.xml --days=2",
    "visionplus_id": "epg-grabber --config=./config/visionplus.id.config.js --channels=./config/visionplus.id.channels.xml --output=./guide/visionplus.id.xml --days=4",
    "mncvision_id": "epg-grabber --config=./config/mncvision.id.config.js --channels=./config/mncvision.id.channels.xml --output=./guide/mncvision.id.xml --days=4 --max-connections=4 --timeout=30000",
    "movistarplus_es": "epg-grabber --config=./config/movistarplus.es.config.js --channels=./config/movistarplus.es.channels.xml --output=./guide/movistarplus.es.xml --days=3"
